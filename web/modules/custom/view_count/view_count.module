<?php
/**
 * Implements hook_preprocess_HOOK() 
 */
function view_count_preprocess_node(&$variables) {

  if (\Drupal::currentUser()->isAnonymous() && $variables['node']->getType() == 'news') {
    $node = $variables['node'];
    $view_count = $node->get('field_view_count')->value + 1;
    $node->set('field_view_count', $view_count);
    $node->save();
  }
}